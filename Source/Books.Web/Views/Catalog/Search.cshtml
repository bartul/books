@{
    ViewData["Title"] = "Catalog Search Page";
}
<h3>Catalog</h3>
<table>
@foreach(var item in Model) {
    <tr>
        <td><img src="@item.ImageUrlMedium" alt="@item.BookTitle"></td>
        <td><a href="@Url.Action("Book", new { id = item.ISBN })">@item.BookTitle</a></td>
    </tr>
}
</ul>